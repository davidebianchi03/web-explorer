{
  "name": "web-explorer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:dev": "node -r dotenv/config dist/server.js",
    "start:watch": "nodemon --legacy-watch -r dotenv/config src/server.ts",
    "start": "npm run build:ts && node ./build/server.js",
    "build:ts": "npx tsc && npm run copy-assets",
    "copy-assets": "copyfiles -u 1 src/pages/**/*.*  build/pages",
    "lint":"tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.16",
    "dotenv": "^16.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "copyfiles": "^2.4.1",
    "eslint": "^8.36.0"
  }
}
