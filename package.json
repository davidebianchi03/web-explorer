{
  "name": "web-explorer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:dev": "node -r dotenv/config dist/server.js",
    "start:watch": "nodemon --legacy-watch -r dotenv/config src/server.ts",
    "start:js": "npm run build:ts && node ./build/server.js",
    "build:ts": "npx tsc && npm run copy-assets",
    "copy-assets": "copyfiles -u 1 src/pages/**/*.*  build/pages",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "prisma:push": "prisma db push",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/ejs": "^3.1.2",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.9",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.0",
    "@types/node": "^20.1.3",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.36.0",
    "nodemon": "^2.0.15",
    "prisma": "^4.11.0",
    "ts-node": "^10.5.0",
    "typescript": "^5.0.2"
  }
}
